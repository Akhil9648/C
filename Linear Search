#include<stdio.h>
int main()
{
	int arr[10],n,i,key,found=0;
	printf("Enter n:");
	scanf("%d",&n);
	printf("\n Enter %d array elements:",n);
	for(i=0;i<n;i++)
	{
		scanf("%d",&arr[i]);
	}
	printf("\nEnter the key:");
	scanf("%d",&key);
	for(i=0;i<n;i++)
	{
		if(arr[i]==key)
		{
			printf("\nKey found at index=%d",i);
			found=1;
			break;
			
		}
	}
	if(found==0)
	{
		printf("\n Key does not exist");
	}
	return 0;
}
