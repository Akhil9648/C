// Linked List representation of stack
// Online C compiler to run C program online
#include <stdio.h>
#include<stdlib.h>
#define SIZE 10
struct Node{
    char ch;
    struct Node*next;
};
struct stack{
    struct Node* top;
};
void init(struct stack*sptr)
{
    sptr->top=NULL;
}
int push(struct stack*sptr,char ch)
{
    struct Node*ptr=(struct Node*)malloc(sizeof(struct Node*));
    if(ptr==NULL)
    {
        return 0;
    }
    else
    {
        ptr->ch=ch;
        ptr->next=sptr->top;
        sptr->top=ptr;
        return 1;
    }
}
char pop(struct stack*sptr)
{
    if(sptr->top==NULL)
    {
        return 0;
    }
    else
    {
        struct Node*ptr;
        ptr=sptr->top;
        char ch=ptr->ch;
        sptr->top=ptr->next;
        free(ptr);
        return ch;
    }
}
int main() {
    char name[SIZE];
    gets(name);
    struct stack*st;
    init(st);
    int i=0;
    while(name[i]!=NULL)
    {
        push(st,name[i]);
        i++;
    }
    i=0;
    char ch=pop(st);
    while(ch!=NULL)
    {
        name[i]=ch;
        i++;
        ch=pop(st);
    }
    puts(name);
    return 0;
}
