#include<stdio.h>
int main()
{
	int n,i,sum;
	printf("Enter a number:");
	scanf("%d",&n);
	int arr[n];
	printf("\n Enter a marks of %d students:",n);
	for(i=0;i<n;i++)
	{
		scanf("%d",&arr[i]);
	}
	sum=0;
	printf("\n Entered marks:");
	for(i=0;i<n;i++)
	{
		printf("%d,",arr[i]);
		sum=sum+arr[i];
	}
	printf("\n Average marks=%f",(float)sum/n);
	return 0;
}
